  "scripts": {
    "dev:plugin": "concurrently \"nx serve copy-paste\" \"tsc -w -p apps/copy-paste/plugin/tsconfig.json\"",
    "dev:copy-paste": "concurrently -n plugin,next 'nx run copy-paste:build:plugin --watch' 'nx serve copy-paste'",
    "start:copy-paste": "nx serve copy-paste",
    "build:copy-paste": "concurrently -n plugin,next 'nx run copy-paste:build:plugin' 'nx run copy-paste:build'",
    "lint:copy-paste": "nx run copy-paste:lint",
    "test:copy-paste": "nx run copy-paste:test",
    "build:plugin:copy-paste": "esbuild apps/copy-paste/plugin/index.ts --bundle --target=es6 --outfile=dist/apps/copy-paste/plugin.js",
    "build:next:copy-paste": "nx run copy-paste:build",
    "watch:copy-paste": "concurrently -n plugin,next 'nx run copy-paste:build:plugin --watch' 'nx serve copy-paste --dev'",
    "storybook": "npx nx run components:storybook",
    "build-storybook": "npx nx run components:build-storybook",
    "test-storybook": "npx test-storybook --url http://localhost:4400/ --coverage --config-dir packages/components/.storybook",
    "chromatic": "npx chromatic",
    "dev:portfolio": "npx nx serve portfolio --dev",
    "neurotales:dev": "npx nx serve neurotales",
    "matthewhuntsberry:dev": "npx nx serve matthewhuntsberry",
    "some-peoples-kids:dev": "npx nx serve some-peoples-kids",
    "dupe": "git archive -o ghostkit.zip HEAD"
  },
